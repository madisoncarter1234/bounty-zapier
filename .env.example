# Comma-separated Zapier/Make webhook URLs
WEBHOOK_URLS=

# Polling interval in seconds (default: 60)
POLL_INTERVAL=60

# Comma-separated tags to filter (optional, empty = all)
FILTER_TAGS=

# Minimum reward in USDC to trigger webhooks (default: 0 = all)
FILTER_MIN_REWARD=0

# Server port (default: 3001)
PORT=3001
